                "description": "get string by ID",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "contacts"
                ],
                "summary": "Show list of contacts",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Contact Name search",
                        "name": "q",
                        "in": "query"
